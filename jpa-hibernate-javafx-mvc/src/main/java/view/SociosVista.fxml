   <children>
      <Button fx:id="btnConsultar" layoutX="20.0" layoutY="127.0" mnemonicParsing="false" onAction="#consultSocios" prefHeight="26.0" prefWidth="138.0" text="Consultar" />
      <Label layoutX="244.0" layoutY="27.0" text="Socios">
